    <ImageView
        android:id="@+id/banner"
        android:layout_width="wrap_content"
        android:layout_height="206dp"
        android:src="@drawable/banner"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.003" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/banner"
        app:layout_constraintVertical_bias="0.072">

        <ImageButton
            android:id="@+id/imgBtnSopa"
            android:layout_width="125dp"
            android:layout_height="125dp"
            android:background="@drawable/segundo" />

        <ImageButton
            android:id="@+id/imgBtnSegundo"
            android:layout_width="125dp"
            android:layout_height="125dp"
            android:background="@drawable/segundo" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        app:layout_constraintVertical_bias="0.004">

        <ImageButton
            android:id="@+id/imgBtnExtra"
            android:layout_width="125dp"
            android:layout_height="125dp"
            android:background="@drawable/segundo" />

        <ImageButton
            android:id="@+id/imgBtnGaseosa"
            android:layout_width="125dp"
            android:layout_height="125dp"
            android:background="@drawable/gaseosas" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
        app:layout_constraintVertical_bias="0.0">

        <ImageButton
            android:id="@+id/imgBtnRefresco"
            android:layout_width="125dp"
            android:layout_height="125dp"
            android:background="@drawable/gaseosas" />

        <ImageButton
            android:id="@+id/imgBtnCerveza"
            android:layout_width="125dp"
            android:layout_height="125dp"
            android:background="@drawable/gaseosas" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4"
        app:layout_constraintVertical_bias="0.0">

        <ImageButton
            android:id="@+id/buttonPostres"
            android:layout_width="125dp"
            android:layout_height="125dp"
            android:background="@drawable/postres" />

        <ImageButton
            android:id="@+id/buttonAcerca"
            android:layout_width="125dp"
            android:layout_height="125dp"
            android:background="@drawable/gaseosas" />

    </LinearLayout>


    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/adminbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/btn"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.954"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/banner"
        app:layout_constraintVertical_bias="0.023"
        app:srcCompat="@drawable/baseline_admin_panel_settings_24" />


